<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane fx:id="pane" prefHeight="750" prefWidth="1200" styleClass="mainMenu" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MainMenuPageController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="203.0" layoutY="119.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="566.0" prefWidth="840.0" spacing="30.0" stylesheets="@../css/styleSheet.css">
         <children>
            <Button fx:id="newgame" mnemonicParsing="false" onAction="#newGame" prefHeight="91.0" prefWidth="148.0" text="new game">
               <font>
                  <Font name="Javanese Text" size="18.0" />
               </font></Button>
            <Button mnemonicParsing="false" onAction="#continueGame" prefHeight="81.0" prefWidth="139.0" text="continue game" />
            <Button fx:id="profle" mnemonicParsing="false" onAction="#profile" prefHeight="79.0" prefWidth="148.0" text="profile">
               <font>
                  <Font name="Javanese Text" size="18.0" />
               </font></Button>
            <Button fx:id="scoreboard" mnemonicParsing="false" onAction="#scoreboard" prefHeight="85.0" prefWidth="148.0" text="scoreboard">
               <font>
                  <Font name="Javanese Text" size="18.0" />
               </font></Button>
            <Button mnemonicParsing="false" onAction="#quitGame" prefHeight="58.0" prefWidth="119.0" text="Quit Game" />
         </children>
      </VBox>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="196.0" layoutY="22.0" prefHeight="65.0" prefWidth="840.0" style="-fx-font-size: 20;" stylesheets="@../css/style.css" text="Main Menu" />
      <Button fx:id="back" layoutX="14.0" layoutY="24.0" mnemonicParsing="false" onAction="#back" prefHeight="20.0" prefWidth="62.0" stylesheets="@../css/styleSheet.css" text="Back" />
   </children>
</Pane>
